<h1>Home</h1>

<div class="section">
   <h2>List of array elements</h2>
   {{#each model.elements as |element|}}
   <p>{{element}}</p>
   {{/each}}
</div>

<div class="section">
   <h2>List of users</h2>
   {{#each model.users as |user|}}
   <div class="well">
      <h2>{{user.name}}</h2>
      <p>{{user.email}}</p>
      <p>{{user.username}}</p>
   </div>
   {{/each}}
</div>

<div class="section">
   <h2>Another section</h2>
   {{list-of-posts class="list-of-posts" title="List of posts" posts=model.posts}}
</div>

{{outlet}}
